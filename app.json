{
    "expo": {
        "name": "front-end",
        "slug": "front-end",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/images/adaptive-icon.png",
                "backgroundColor": "#F5E6CC"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./src/assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./src/assets/images/splash-icon-light.png",
                    "imageWidth": 200,
                    "resizeMode": "cover",
                    "backgroundColor": "#F5E6CC"
                }
            ],
            [
                "expo-font",
                {
                    "fonts": [
                        "./src/app/assets/fonts/Oxygen-Light.ttf",
                        "./src/app/assets/fonts/Oxygen-Regular.ttf",
                        "./src/app/assets/fonts/Oxygen-Bold.ttf"
                    ]
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
